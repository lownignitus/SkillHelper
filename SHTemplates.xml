<!-- Skill Helper v3.2.19 -->


<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<!-- Templates -->

	<!-- Basic Frame -->
	<Frame name="shTBasicFrame" frameStrata="BACKGROUND" clampedToScreen="true" virtual="true">
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="3" right="3" top="3" bottom="3" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="16" />
			</EdgeSize>
			<!-- Color -->
		</Backdrop>
	</Frame>

	<!-- Title Text -->
	<FontString name="shTTitleText" font="Fonts\FRIZQT__.TTF" justifyH="CENTER" justifyV="MIDDLE" parentKey="text" virtual="true">
		<FontHeight val="14" />
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER" relativeTo="$parent" />
		</Anchors>
	</FontString>

</Ui>